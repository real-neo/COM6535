let post = $('#post_<%= raw @post_id %>');
if(post.length)
    post.find('#poll_btn').html('<%=raw @poll_buttons %>')
else
    $('#poll_btn').html('<%= raw @poll_buttons %>')

<%- if @poll_option_record.nil? %>
  $.flashAlert('Your vote has been removed!', 'notice')
<%- else %>
  $.flashAlert('Your vote has been submitted!', 'notice')
<%- end %>
